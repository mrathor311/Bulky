@model List<Category>


<h1>Categaory List</h1>
<table class="table table-bordered table-striped">
	<thead>
	<tr>
		<th>
			Category Name
		</th>
		<th>
			Order Display 
		</th>
     </tr>
	</thead>
	<tbody>

		@foreach(var obj in Model){
		<tr>
<td>@obj.Name
</td>
<td>
	@obj.DisplayOrder
</td>
		</tr>
		}




	</tbody>
</table>